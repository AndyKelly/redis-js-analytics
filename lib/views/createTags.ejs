<!doctype html>
<html>
<head>
	<title>Move Challenge - Admin Panel</title>
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
	<style>
		body 		{ padding-top:80px; }
	</style>
</head>
<body>
<div class="container">


	<div class="page-header text-center">
		<h1><span class="fa fa-anchor"></span> Tags Page</h1>
		<a href="/logout" class="btn btn-default btn-sm">Logout</a>
	</div>

	<div class="row">

		<!-- LOCAL INFORMATION -->
		<div class="col-sm-6">
			<div class="well">
				<h3>Tag Creation</h3>
				<h4>Here's some info from the server:</h4>
				
				<% function tag(tag) { %>
					<div>

							<strong>Name: <%= tag.displayName %>
							</br>
							Parents: <%= tag.parents %>
							</br>
							ID: <%= tag.id %>
							</br>

					</div>
							</br>
							</br>
				<% } %>

				<% tags.map(tag) %>
				
				
			</div>
		</div>

	</div>


</div>
</body>
</html>
